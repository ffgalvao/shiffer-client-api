{
  "name": "fgalvao/shiffle-client-api",
  "description": "Api client for Shiffle",
  "homepage": "https://github.com/ffgalvao/shiffle-client-api",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Felippe Ladeira",
      "email": "ffgalvao@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.3",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3"
  },
  "autoload": {
    "psr-0": {
      "Shiffle": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "fgalvao\\Shiffle\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
